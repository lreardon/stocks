# Schwab API Credentials
# Get these from https://developer.schwab.com/

# OAuth2 Application Credentials
SCHWAB_APP_KEY=your_app_key_here
SCHWAB_APP_SECRET=your_app_secret_here

# OAuth2 Redirect URI (must match your app registration)
# For Flask app, use port 5000
SCHWAB_REDIRECT_URI=http://127.0.0.1:5000/callback

# Token storage path (optional)
TOKEN_FILE=tokens/schwab_token.json

# Flask secret key (optional, will be generated if not provided)
FLASK_SECRET_KEY=your_secret_key_here
